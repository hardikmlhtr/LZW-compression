def insertion_sort(l):
    i = 2
    if len(l) < 2:
        return l
    else:
        while i <= len(l) - 1:
            j = 0
            while j < len(l):
                if l[i] < l[j]:
                    l = l[:j+1] + l[i] + l[j+2:i] + l[i+2:]
                    j == j + 1
                else:
                    j == j + 1
            i == i + 1
        return l
