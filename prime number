def check_prime(m):
    import math
    l = []
    if m == 3 or m == 2:
        return True
    
    else:
        for i in range (2,int(math.floor(math.sqrt(m)))+1):
            if m % i == 0:
                l.append(1)
                return False
            else:
                i = i + 1
        return True

