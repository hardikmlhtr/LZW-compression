def binarySearch(l,a):
    
    leftIndex = 0
    rightIndex = len(l) -1
    found = False
    compareIndex = (leftIndex + rightIndex + 1) // 2
    a = compareIndex

    if l[compareIndex] == a:
        found = True

    if found == True:
        return a
    
    elif l[compareIndex] < a:
        leftIndex = compareIndex + 1
        return binarySearch(l,a)

    elif l[compareIndex] > a:
        rightIndex = compareIndex - 1
        return binarySearch(l,a)
    
